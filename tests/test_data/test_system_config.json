{
  "browser": {
    "browser_type": "chrome",
    "headless": true,
    "timeout_seconds": 30,
    "max_retries": 3,
    "required_login_domains": ["seerfar.cn", "www.maozierp.com"],
    "debug_port": 9222
  },
  "selector_filter": {
    "store_min_sales_30days": 500000.0,
    "store_min_orders_30days": 250,
    "item_category_blacklist": ["电子产品", "Electronics", "数码配件"],
    "max_products_to_check": 100
  },
  "performance": {
    "max_concurrent_tasks": 5,
    "retry_count": 3,
    "enable_cache": true,
    "cache_ttl": 3600,
    "batch_size": 10
  },
  "logging": {
    "log_level": "INFO",
    "max_log_files": 10,
    "log_file_size_mb": 50
  },
  "debug_mode": false,
  "dryrun": false
}